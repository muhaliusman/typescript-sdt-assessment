{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globalTeardown": "./jest-e2e.teardown.ts",
  "globalSetup": "./jest-e2e.setup.ts",
  "moduleNameMapper": {
    "^(docs|workers|utils|services|entities|config|controllers|middleware|routes|repositories|schemas|exceptions|queues|cron)/(.*)$": "<rootDir>/../src/$1/$2"
  }
}
